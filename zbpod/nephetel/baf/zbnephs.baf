IF
    AreaCheck("OH8100")  // TBP2 - Staging Area Map
    Global("ZB_INTERVIEW","GLOBAL",1)
    Range([PC],30)
    See([PC])
THEN
    RESPONSE #100
    SetInterrupt(FALSE)
    SetGlobal("ZB_INTERVIEW","GLOBAL",2)
    FaceObject([PC])
    SmallWait(5)
    StartDialogueNoSet([PC])
    SetInterrupt(TRUE)
END
