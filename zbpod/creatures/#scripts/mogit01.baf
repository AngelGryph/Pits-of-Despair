  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",6)
    SetGlobal("mindblast","LOCALS",1)
    SetInterrupt(FALSE) 
    ForceSpell(LastSeenBy(Myself),PSIONIC_MIND_BLAST)  // SPIN775.SPL (Psionic Blast)
    SetInterrupt(TRUE) 
END

