/////////////////////////////////////////////////////////////////////
// Initial Set-Up
/////////////////////////////////////////////////////////////////////
IF
  Global("Prep","LOCALS",0)
  See([GOODCUTOFF])
  Allegiance(Myself,ENEMY)
  LevelLT(Myself,21)
THEN
  RESPONSE #100
    SetGlobal("Prep","LOCALS",1)
    SetGlobal("MO_Rest","LOCALS",1)
    GiveItemCreate("POTN55",Myself,1,0,0)     // potion of superior healing
    Continue()
END

IF
  Global("Prep","LOCALS",0)
  See([GOODCUTOFF])
  Allegiance(Myself,ENEMY)
  LevelLT(Myself,25)
  LevelGT(Myself,20)
THEN
  RESPONSE #100
    SetGlobal("Prep","LOCALS",1)
    SetGlobal("MO_Rest","LOCALS",1)
    ReallyForceSpell(Myself,PSIONIC_INERTIAL_BARRIER)
    GiveItemCreate("POTN55",Myself,1,0,0)     // potion of superior healing
    Continue()
END

IF
  Global("Prep","LOCALS",0)
  See([GOODCUTOFF])
  Allegiance(Myself,ENEMY)
  LevelGT(Myself,24)
THEN
  RESPONSE #100
    SetGlobal("Prep","LOCALS",1)
    SetGlobal("MO_Rest","LOCALS",1)
    ReallyForceSpell(Myself,PSIONIC_INERTIAL_BARRIER)
    ReallyForceSpell(Myself,WARRIOR_HARDINESS)
    GiveItemCreate("POTN55",Myself,1,0,0)     // potion of superior healing
    Continue()
END

/////////////////////////////////////////////////////////////////////
// Assign Warrior HLA
/////////////////////////////////////////////////////////////////////
IF
  Global("Prep","LOCALS",1)
  Global("MO_AssignHLA","LOCALS",0)
  Allegiance(Myself,ENEMY)
  DifficultyGT(HARD)
  LevelGT(Myself,20)
THEN
  RESPONSE #100
    SetGlobal("MO_AssignHLA","LOCALS",1)
    SetGlobal("MO_GWW","LOCALS",1)
    Continue()
  RESPONSE #100
    SetGlobal("MO_AssignHLA","LOCALS",1)
    SetGlobal("MO_Crit","LOCALS",1)
    Continue()
  RESPONSE #100
    SetGlobal("MO_AssignHLA","LOCALS",1)
    SetGlobal("MO_Power","LOCALS",1)
    Continue()
END

/////////////////////////////////////////////////////////////////////
// Rest
/////////////////////////////////////////////////////////////////////
IF
  Global("MO_Rest","LOCALS",1)
  !Detect(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    SetGlobal("MO_Rest","LOCALS",2)
    SetGlobalTimer("MO_RestTimer","LOCALS",EIGHT_HOURS)
    Continue()
END

IF
  Global("MO_Rest","LOCALS",2)
  Detect(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    SetGlobal("MO_Rest","LOCALS",1)
    Continue()
END

IF
  Global("MO_Rest","LOCALS",2)
  GlobalTimerExpired("MO_RestTimer","LOCALS")
THEN
  RESPONSE #100
    SetGlobal("MO_Rest","LOCALS",0)
    SetGlobal("Prep","LOCALS",0)
    SetGlobal("MO_PrepHLA","LOCALS",0)
    SetGlobal("MO_AssignHLA","LOCALS",0)
    SetGlobal("MO_GWW","LOCALS",0)
    SetGlobal("MO_Crit","LOCALS",0)
    SetGlobal("MO_Power","LOCALS",0)
    ApplySpell(Myself,RESTORE_FULL_HEALTH)  // SPIN746.SPL (No such index)
    Rest()
END

/////////////////////////////////////////////////////////////////////
// Heal!
/////////////////////////////////////////////////////////////////////
IF
  !GlobalTimerNotExpired("MO_SpellCastTimer","LOCALS")
  HasItem("POTN55",Myself)
  HPPercentLT(Myself,60)
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",6)
    UseItem("POTN55",Myself)  // ~Potion of Superior Healing~
END

IF
  !GlobalTimerNotExpired("MO_SpellCastTimer","LOCALS")
  HasItem("POTN52",Myself)
  HPPercentLT(Myself,60)
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",6)
    UseItem("POTN52",Myself)  // ~Potion of Extra Healing~
END

/////////////////////////////////////////////////////////////////////
// Hunt the Player
/////////////////////////////////////////////////////////////////////
IF
  !Detect([GOODCUTOFF])
  Allegiance(Myself,ENEMY)
  OR(2)
    Global("Prep","LOCALS",1)
    HitBy([GOODCUTOFF],CRUSHING)
  !GlobalTimerNotExpired("MO_IsMovingTimer","LOCALS")
  CheckStatLT(Player1,1,SANCTUARY)
THEN
  RESPONSE #100
