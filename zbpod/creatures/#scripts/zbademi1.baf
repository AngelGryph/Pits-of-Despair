/////////////////////////////////////////////////////////////////////
// Ten Rounds Buff Up and Timestop
/////////////////////////////////////////////////////////////////////
IF
  !GlobalTimerNotExpired("DemiTS","LOCALS")
  See(NearestEnemyOf(Myself))
  DifficultyGT(HARD)
THEN
  RESPONSE #100
    SetGlobalTimer("DemiTS","LOCALS",60)
    ForceSpell(Myself,WIZARD_TIME_STOP)  // SPWI909.SPL (Time Stop)
    SetGlobalTimer("MO_TS","GLOBAL",18)
    ReallyForceSpell(Myself,WIZARD_TELEPORT_FIELD)  // SPWI421.SPL (Teleport Field)
    ReallyForceSpell(Myself,WIZARD_PROTECTION_FROM_MAGIC_WEAPONS)  
    ReallyForceSpell(Myself,WIZARD_SPELL_SHIELD)  // SPWI519.SPL (Spell Shield)
    ReallyForceSpell(Myself,WIZARD_PROTECTION_FROM_FIRE)
END

